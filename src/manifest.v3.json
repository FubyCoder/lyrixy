{
    "description": "A spotify extension for additional lyrics",
    "manifest_version": 3,
    "name": "Lyrixy",
    "version": "0.0.4",
    "content_scripts": [
        {
            "matches": ["https://open.spotify.com/*"],
            "js": ["index.js"],
            "css": ["index.css"],
            "run_at": "document_end"
        }
    ],
    "host_permissions": ["https://lrclib.net/*"]
}
